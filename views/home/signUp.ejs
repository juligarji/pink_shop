          <!-- Aqui va el contenedor de los productos -->
    <div class = "row">

        <div class="container">
          <div class="input-field col l6 m6 s11">
              <input name = "name" id="name" type="email" class="validate">
              <label for="email">Nombres</label>
          </div>
          <div class="input-field col l6 m6 s11">
            <input name = "password" id="password" type="password" class="validate">
              <label for="password">Apellidos</label>
