                      <h1>aqui va el contenido</h1>
                      <div class = "divider"></div>


                    <div class = "container col l12 m12 s12 z-depth-3">
                      <div class = "row">
                      <form method = 'post' onsubmit = "uploadCategory(event)">
                        <table class="responsive-table">
                            <thead>
                              <tr>
                                <th>Tipo</th>
                                <th>Nombre</th>
                                <th>Descuento</th>
                                <th>Descripcion</th>

                              </tr>
                            </thead>
                            <tbody>
                              <tr>
                                <td>
                                  <div class="input-field col l12 m12 s12">
                                    <select name = "kind">
                                      <option value="" disabled selected>Tipo</option>
                                      <option value="Tamaño">Tamaño</option>
                                      <option value="Fragancia">Fragancia</option>
                                      <option value="Temporada">Temporada</option>
                                      <option value="Marca">Marca</option>
                                    </select>
                                  </div>
                                </td>
                                <td>
                                    <div class="input-field col l12 m12 s12">
                                      <input name = "name" placeholder="Nombre(Unico)" type="text" class="validate">
                                    </div>
                                </td>
                                <td>
                                  <div class="input-field col l12 m12 s12">
                                    <input name = "discount" placeholder="Descuento" value='0' type="number" class="validate">
                                  </div>
                                </td>
                                <td>
                                  <div class="input-field col s12 m12 l12">
                                    <textarea name = "description" class="materialize-textarea"></textarea>
                                  </div>
                                </td>

                              </tr>
                            </tbody>
                        </table>
                          <div class = "col l12">
                            <button class="btn btn-large waves-effect waves-light green"><i class="material-icons">add</i></button>
                          </div>

                        </form>
                      </div>

                        <div class = "row">
                          <div class = "divider"></div>
                          <h1 class = 'center'>aqui va el contenido</h1>
                          <div class = "divider"></div>

                          <table id = "categoriesContainer" class="bordered responsive-table">
                            <thead>
                              <tr>
                                <th>Tipo</th>
                                <th>Nombre</th>
                                <th>Descuento</th>
                                <th>Descripcion</th>
                                <th>Acciones</th>
                              </tr>
                            </thead>
                            <tbody>
                              <% list.forEach(function(element){ %>
                              <tr value = "<%=element.name%>">
                                <td><%=element.kind%></td>
                                <td><%=element.name%></td>
                                <td><%=element.disscount%></td>
                                <td><%=element.description%></td>
                                <td>
                                  <a onclick = "editCategory('<%=element.name%>')" class="btn-floating btn-small waves-effect waves-light blue">
                                    <i class="material-icons">
                                      edit
                                    </i>
                                  </a>
                                  <a onclick = "deleteCategory('<%=element.name%>')" class="btn-floating btn-small waves-effect waves-light red">
                                    <i class="material-icons">
                                      close
                                    </i>
                                  </a>

                                </td>
                              </tr>
                              <% }); %>

                            </tbody>
                          </table>
                        </div>
